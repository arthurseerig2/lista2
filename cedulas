#include <stdio.h>
int main() {
	int valor, total;
	printf("Digite um valor em reais: ");
	scanf("%d",&valor);
	total = (int)valor/100;
	printf("\n%d cedulas de 100\n", total);
	valor = valor-(total*100);
	total = (int)valor/50;
	printf("%d cedulas de 50\n", total);
	valor = valor-(total*50);
	total = (int)valor/20;
	printf("%d cedulas de 20\n", total);
	valor = valor-(total*20);
	total = (int)valor/10;
	printf("%d cedulas de 10\n", total);
	valor = valor-(total*10);
	total = (int)valor/5;
	printf("%d cedulas de 5\n", total);
	valor = valor-(total*5);
	total = (int)valor/2;
	printf("%d cedulas de 2\n", total);
	valor = valor-(total*2);
	total = (int)valor/1;
	printf("%d moeda de 1\n", total);

	return 0;

}
